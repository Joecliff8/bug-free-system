.form-control__invalid:hover { border-color: rgb(189, 40, 55); }

.form-control--square { max-width: 48px; }

.form-control-sm { padding-left: 0.4375rem; padding-right: 0.4375rem; }

.form-check { position: relative; }

.form-check-label { color: rgb(98, 98, 98); cursor: pointer; display: block; font-size: 14px; font-weight: 400; min-height: 20px; min-width: 20px; padding-left: 28px; transition: all 0.25s ease 0s; }

.rtl .form-check-label { padding-right: 28px; padding-left: 0px; }

.form-check-label > * { display: block; }

.form-check-label > :first-child { color: rgb(51, 51, 51); font-weight: 600; line-height: 24px; }

.form-check-label > :first-child + * { margin-top: 4px; }

.form-check-label > :only-child { font-weight: 400; }

.form-check-label::before { background: 0px 8px no-repeat rgb(255, 255, 255); border: 1px solid rgb(169, 169, 169); border-radius: 4px; content: ""; height: 20px; left: 0px; position: absolute; top: 2px; transition: all 0.25s ease 0s; width: 20px; }

.rtl .form-check-label::before { right: 0px; }

.form-check .labelError::before { border-color: rgb(227, 57, 77); }

.form-check .checkboxIcon { color: rgb(255, 255, 255); height: 16px; left: 2px; opacity: 0; pointer-events: none; position: absolute; top: 4px; transition: opacity 0.25s ease 0s; width: 16px; z-index: -1; }

.rtl .form-check .checkboxIcon { right: 2px; }

.form-check-input { position: absolute; margin-left: 0px; opacity: 0; transition: border-color 0.25s ease 0s, background-color 0.25s ease 0s; }

.rtl .form-check-input { margin-right: 0px; }

@media (hover: hover) {
  .form-check-input ~ .form-check-label:hover::before { border-color: rgb(0, 165, 239); }
  .form-check-input ~ .form-check-label:hover ~ .checkboxIcon { color: rgb(209, 236, 250); opacity: 1; z-index: 1; }
}

.form-check-input:checked ~ .form-check-label::before, .form-check-input:not(.radio):indeterminate ~ .form-check-label::before { background-color: rgb(0, 165, 239); border-color: rgb(0, 165, 239); }

.form-check-input:checked ~ .checkboxIcon, .form-check-input:not(.radio):indeterminate ~ .checkboxIcon { color: rgb(255, 255, 255); opacity: 1; z-index: 1; }

@media (hover: hover) {
  .form-check-input:checked + .form-check-label:hover::before, .form-check-input:not(.radio):indeterminate + .form-check-label:hover::before { background-color: rgb(61, 184, 245); border-color: rgb(61, 184, 245); }
  .form-check-input:checked + .form-check-label:hover ~ .checkboxIcon, .form-check-input:not(.radio):indeterminate + .form-check-label:hover ~ .checkboxIcon { color: rgb(255, 255, 255); }
}

.form-check-input:disabled ~ .form-check-label { cursor: not-allowed; color: rgb(169, 169, 169) !important; }

.form-check-input:disabled ~ .form-check-label::before { background-color: rgb(248, 248, 248); border-color: rgb(216, 216, 216); }

.form-check-input:disabled ~ .form-check-label ~ .checkboxIcon { opacity: 0; }

.form-check-input:disabled ~ .form-check-label > :first-child { color: inherit; }

.form-check-input:checked:disabled ~ .checkboxIcon { color: rgb(216, 216, 216); opacity: 1; }

.form-check-input:checked:disabled ~ .form-check-label::before { background-color: rgb(248, 248, 248); border-color: rgb(216, 216, 216); }

.form-check-input:checked:disabled ~ .form-check-label:hover + .checkboxIcon { color: rgb(216, 216, 216); }

