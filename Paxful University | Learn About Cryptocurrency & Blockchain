.popover, .popover .arrow { position: absolute; display: block; }

.popover .arrow { width: 12px; height: 6px; margin: 0px 4px; }

.popover .arrow::after, .popover .arrow::before { position: absolute; display: block; content: ""; border-color: transparent; border-style: solid; }

.bs-popover-auto[x-placement^="top"], .bs-popover-top { margin-bottom: 6px; }

.bs-popover-auto[x-placement^="top"] > .arrow, .bs-popover-top > .arrow { bottom: -7px; }

.bs-popover-auto[x-placement^="top"] > .arrow::before, .bs-popover-top > .arrow::before { bottom: 0px; border-width: 6px 6px 0px; border-top-color: rgb(216, 216, 216); }

.bs-popover-auto[x-placement^="top"] > .arrow::after, .bs-popover-top > .arrow::after { bottom: 1px; border-width: 6px 6px 0px; border-top-color: rgb(255, 255, 255); }

.bs-popover-auto[x-placement^="right"], .bs-popover-right { margin-left: 6px; }

.bs-popover-auto[x-placement^="right"] > .arrow, .bs-popover-right > .arrow { left: -7px; width: 6px; height: 12px; margin: 4px 0px; }

.bs-popover-auto[x-placement^="right"] > .arrow::before, .bs-popover-right > .arrow::before { left: 0px; border-width: 6px 6px 6px 0px; border-right-color: rgb(216, 216, 216); }

.bs-popover-auto[x-placement^="right"] > .arrow::after, .bs-popover-right > .arrow::after { left: 1px; border-width: 6px 6px 6px 0px; border-right-color: rgb(255, 255, 255); }

