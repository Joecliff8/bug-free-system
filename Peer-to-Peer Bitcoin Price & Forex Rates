.react-app .switcher { display: block; margin: 0px; position: relative; }

.react-app .switcher-input { display: block; inset: 0px; cursor: pointer; height: 0px; margin: 0px; opacity: 0; position: absolute; width: 0px; }

.react-app .switcher-input + span { background: rgb(216, 216, 216); border-radius: 16px; cursor: pointer; display: block; height: 16px; padding: 0px 2px; position: relative; transition: background-color 125ms ease-in-out 0s, border-color 125ms ease-in-out 0s; user-select: none; width: 28px; }

.react-app .switcher-input + span::before { background: rgb(255, 255, 255); border-radius: 50%; content: ""; display: block; height: 12px; left: 2px; position: absolute; top: 50%; transform: translateY(-50%); transition: left 125ms ease-in-out 0s, border-color 125ms ease-in-out 0s, background-color 125ms ease-in-out 0s; will-change: left; width: 12px; }

.react-app .switcher-input:hover + span { background-color: rgb(169, 169, 169); }

.react-app .switcher-input:checked + span { background-color: rgb(0, 165, 239); }

.react-app .switcher-input:checked + span::before { left: calc(100% - 14px); }

.react-app .switcher-input:checked + span:hover::before { background-color: rgb(209, 236, 250); }

.react-app .switcher-input:disabled + span { background-color: rgb(238, 238, 238); }

.react-app .switcher-input:disabled + span::before { background-color: rgb(255, 255, 255) !important; }

.react-app .isError .switcher-input:not(:disabled) + span { background-color: rgb(227, 57, 77); }

.react-app .isError .switcher-input:not(:disabled):hover + span::before { background-color: rgb(251, 228, 231); }

.react-app .form-radio { padding-left: 0px; }

.react-app .form-radio .form-check-label::before { border-radius: 50%; transition: border-color 0.25s ease 0s, background-color 0.25s ease 0s; }

.react-app .rtl .form-radio .form-check-label::before { right: 0px; }

.react-app .form-radio .form-check-label::after { background-color: rgb(216, 216, 216); border-radius: 50%; content: ""; display: block; height: 8px; left: 6px; opacity: 0; position: absolute; top: 8px; transform: scale(0); transition: opacity 0.25s ease 0s, transform 0.25s ease 0s; width: 8px; }

.react-app .rtl .form-radio .form-check-label::after { right: 6px; }

.react-app .form-radio .labelError::before { border-color: rgb(227, 57, 77); }

.react-app .form-radio .form-check-input { opacity: 0; }

@media (hover: hover) {
  .react-app .form-radio .radio:not(:disabled) ~ .form-check-label:hover::after { background-color: rgb(209, 236, 250); opacity: 1; transform: scale(1); }
}

